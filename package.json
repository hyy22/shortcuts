{
  "name": "@hyy22/shortcuts",
  "version": "0.0.4",
  "description": "some usable shortcuts run by cli",
  "main": "src/index.js",
  "bin": {
    "shortcuts": "src/index.js"
  },
  "scripts": {
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs --fix",
    "prepare": "husky install",
    "md": "node build/md.js"
  },
  "keywords": [
    "image generate",
    "open url by alias",
    "quick url visit",
    "generate markdown file with front matter",
    "生成自定义尺寸图片",
    "url别名",
    "命令行快速访问url"
  ],
  "author": "hyy22",
  "license": "MIT",
  "lint-staged": {
    "*.{js,jsx,cjs,mjs}": "eslint --cache --fix",
    "*.{js,css,md}": "prettier --write"
