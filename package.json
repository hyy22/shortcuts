  "dependencies": {
    "@vitalets/google-translate-api": "^8.0.0",
    "canvas": "^2.10.1",
    "chalk": "^4.1.2",
    "commander": "^8.2.0",
    "fs-extra": "^10.1.0",
    "open": "^8.4.0",
    "ora": "5.4.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1"
  },
  "engines": {
    "node": ">=14"
